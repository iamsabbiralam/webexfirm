package user

type User struct {
	ID          string  `pb:"1"`
	FullName       string `pb:"2"`
	Email       string `pb:"3"`
}

type CreateUserRequest struct {
	User User `pb:"1"`
}

type CreateUserResponse struct {
	ID string `pb:"1"`
}


type UserService interface {
	CreateUser(CreateUserRequest) CreateUserResponse
}
